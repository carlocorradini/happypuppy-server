{
  "name": "happy_puppy_server",
  "version": "1.0.0",
  "description": "Happy Puppy Server",
  "homepage": "https://github.com/carlocorradini/HappyPuppyServer",
  "bugs": {
    "url": "https://github.com/carlocorradini/HappyPuppyServer/issues",
    "email": "carlo.corradini@studenti.unitn.it"
  },
  "main": "./app.js",
  "private": true,
  "scripts": {
    "clean": "npx rimraf build/ && npx rimraf log/",
    "update": "npx ncu -u && npm install",
    "prettify": "npx prettier --write ./src/**/*.*",
    "prettify:staged": "npx prettier --write",
    "lint": "npx eslint --fix ./src/**/*.*",
    "lint:staged": "npx eslint --fix",
    "prebuild": "npm run clean",
    "build": "npx ttsc --p ./tsconfig.json",
    "postbuild": "npx ts-node ./tools/build",
    "test": "exit 1 || npx jest",
    "test:staged": "exit 1 || npx jest --bail --findRelatedTests",
    "prestart": "npm install --only=prod",
    "start": "npx cross-env NODE_ENV=\"production\" node app.js",
    "prestart:prod": "cd build && npm install --only=prod",
    "start:prod": "cd build && npx cross-env NODE_ENV=\"production\" node app.js",
    "start:dev": "npx ts-node-dev -r tsconfig-paths/register --ignore-watch node_module --respawn --transpileOnly src/app.ts"
  },
  "author": "Carlo Corradini",
  "contributors": [
    "Anthony Farina"
  ],
  "engines": {
    "node": "^10.16.0",
    "npm": "^6.9.0"
  },
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "class-validator": "^0.11.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^3.22.0",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.0.0",
    "reflect-metadata": "^0.1.13",
    "serve-favicon": "^2.5.0",
    "typeorm": "^0.2.24",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^4.4.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.3",
    "@types/helmet": "0.0.45",
    "@types/jest": "^25.1.4",
    "@types/jsonwebtoken": "^8.3.8",
    "@types/node": "^13.9.8",
    "@types/serve-favicon": "^2.5.0",
    "@types/shelljs": "^0.8.7",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "husky": "^4.2.3",
    "jest": "^25.2.4",
    "lint-staged": "^10.1.1",
    "npm-check-updates": "^4.1.0",
    "prettier": "^2.0.2",
    "rimraf": "^3.0.2",
    "shelljs": "^0.8.3",
    "ts-jest": "^25.3.0",
    "ts-node-dev": "^1.0.0-pre.44",
    "tsconfig-paths": "^3.9.0",
    "ttypescript": "^1.5.10",
    "typescript": "^3.8.3",
    "typescript-transform-paths": "^1.1.14"
  }
}
